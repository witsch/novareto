<?xml version="1.0"?>
<object name="portal_skins">

 <object name="documentgallery_images"
    meta_type="Filesystem Directory View"
    directory="nva.galleryview:skins/documentgallery_images"/>
 <object name="documentgallery_styles"
    meta_type="Filesystem Directory View"
    directory="nva.galleryview:skins/documentgallery_styles"/>
 <object name="documentgallery_scripts"
    meta_type="Filesystem Directory View"
    directory="nva.galleryview:skins/documentgallery_scripts"/>

 <skin-path name="*">
  <layer name="documentgallery_images"
     insert-after="custom"/>
  <layer name="documentgallery_styles"
     insert-after="documentgallery_images"/>
  <layer name="documentgallery_scripts"
     insert-after="documentgallery_styles"/>
 </skin-path>

</object>

