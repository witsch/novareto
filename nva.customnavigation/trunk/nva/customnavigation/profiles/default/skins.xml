<?xml version="1.0"?>

<object name="portal_skins">

 <object name="nva_customnavigation_custom_images"
    meta_type="Filesystem Directory View"
    directory="nva.customnavigation:skins/nva_customnavigation_custom_images"/>
 <object name="nva_customnavigation_custom_templates"
    meta_type="Filesystem Directory View"
    directory="nva.customnavigation:skins/nva_customnavigation_custom_templates"/>
 <object name="nva_customnavigation_styles"
    meta_type="Filesystem Directory View"
    directory="nva.customnavigation:skins/nva_customnavigation_styles"/>

 <skin-path name="*">
  <layer name="nva_customnavigation_custom_images"
     insert-after="custom"/>
  <layer name="nva_customnavigation_custom_templates"
     insert-after="nva_customnavigation_custom_images"/>
  <layer name="nva_customnavigation_styles"
     insert-after="nva_customnavigation_custom_templates"/>
 </skin-path>

</object>
