<?xml version="1.0"?>
<object name="Order"
   meta_type="Factory-based Type Information"
   i18n:domain="nva.plone.cart"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
  <property name="title" i18n:translate="">Order</property>
  <property name="description"
    i18n:translate="">Order</property>
  <property name="content_meta_type">Order</property>
  <property name="content_icon">document_icon.gif</property>
  <property name="product">nva.plone.cart</property>
  <property name="factory"></property>
  <property name="immediate_view">atct_edit</property>
  <property name="global_allow">False</property>
  <property name="filter_content_types">False</property>
  <property name="allow_discussion">False</property>
  <alias from="edit" to="atct_edit" />
  <action title="View" action_id="view" category="object" condition_expr=""
	  url_expr="string:${object_url}/@@index" visible="True">
    <permission value="View" />
  </action>
</object>
