<?xml version="1.0"?>
<object name="portal_skins">

 <object name="docgallery_images"
    meta_type="Filesystem Directory View"
    directory="nva.docgallery:skins/docgallery_images"/>
 <object name="docgallery_styles"
    meta_type="Filesystem Directory View"
    directory="nva.docgallery:skins/docgallery_styles"/>

 <skin-path name="*">
  <layer name="docgallery_images"
     insert-after="custom"/>
  <layer name="docgallery_styles"
     insert-after="docgallery_images"/>
 </skin-path>

</object>

